import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scnr = new Scanner(System.in);
        int n;

        System.out.println("Enter an integer:");
        n = scnr.nextInt();
        
        n = Math.abs(n);
        
        System.out.print("Sequence: ");
        for (int i = -n; i <= n; i++) {
            System.out.print(i + " ");
        }
        System.out.println();

        scnr.close();
    }
}
